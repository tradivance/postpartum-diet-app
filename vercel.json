{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "images": {
    "domains": [
      "images.unsplash.com",
      "unsplash.com"
    ],
    "sizes": [640, 828, 1080, 1200, 1920, 2048, 3840],
    "formats": ["image/avif", "image/webp"]
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
